/* ---------------------------------------------------
    CONTENT STYLE
----------------------------------------------------- */

#content {
    width: calc(100% - 265px);
    padding-bottom: 20px;
    min-height: 100vh;
    transition: all 0.3s;
    top:0;
    right: 0;
    margin-left: 250px;
    margin-top: 100px;
}

#content.active {
    width: calc(100% - 95px);
    margin-left: 80px;
}

/* ---------------------------------------------------
    MEDIAQUERIES
----------------------------------------------------- */

@media (max-width: 768px) {
    #sidebar {
